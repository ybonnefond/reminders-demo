{
  "name": "turbulent-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "yarn": ">=1.22.0",
    "node": "=15.x"
  },
  "scripts": {
    "ci": "yarn lint && yarn test:cov",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "lint:eslint": "eslint --cache --color '{src,test,specs}/**/*.ts'",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier -l '{src,test,specs}/**/*.{ts,md,json}'",
    "lint:prettier:fix": "yarn lint:prettier --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.6.15",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/websockets": "^7.6.15",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.22",
    "@types/node": "^14.14.37",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^6.0.0",
    "jest": "26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "socket.io-client": "2.4.0",
    "ts-jest": "26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/test"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ]
  },
  "lint-staged": {
    "*.{ts,js}": [
      "yarn lint:fix"
    ],
    "*.{json,md}": [
      "yarn lint:prettier:fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "deps",
          "deps-dev",
          "semantic-release-config"
        ]
      ]
    }
  }
}
